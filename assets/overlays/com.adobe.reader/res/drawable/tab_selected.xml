<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/backgroundx" />
        </shape>
    </item>
    <item android:bottom="0.0dip"
        android:left="@dimen/bookmark_selected_tab_stroke_offset"
        android:right="@dimen/bookmark_selected_tab_stroke_offset" android:top="@dimen/bookmark_selected_tab_stroke_offset">
        <shape android:shape="rectangle">
            <padding android:bottom="@dimen/bookmark_tab_layout_padding"
                android:top="@dimen/bookmark_tab_layout_padding" />
            <stroke android:width="@dimen/bookmark_selected_tab_stroke_height"
                android:color="@color/bookmark_tab_stroke_color" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
</layer-list>