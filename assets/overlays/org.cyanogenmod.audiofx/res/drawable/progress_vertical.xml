<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="@color/background" android:endColor="@color/background" android:angle="180.0" android:centerY="0.75" android:centerColor="@color/background" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="@color/texttt" android:endColor="@color/texttt" android:angle="180.0" android:centerY="0.75" android:centerColor="@color/texttt" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip android:gravity="bottom" android:clipOrientation="vertical">
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="@color/textt" android:endColor="@color/text" android:angle="180.0" android:centerY="0.75" android:centerColor="@color/textt" />
            </shape>
        </clip>
    </item>
</layer-list>