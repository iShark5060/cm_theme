    <!--require parents-->
    <style name="opt_in_wizard_header_headline" parent="@style/opt_in_wizard_item">
        <item name="android:textSize">@dimen/qp_opt_in_header_headline_text_size</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="opt_in_wizard_header_subheader" parent="@style/opt_in_wizard_item">
        <item name="android:textSize">@dimen/qp_opt_in_header_subheader_text_size</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="opt_in_wizard_header" parent="@style/opt_in_wizard_item">
        <item name="android:textSize">@dimen/qp_opt_in_header_text_size</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:lineSpacingMultiplier">@dimen/qp_h1_line_spacing_multiplier</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="opt_in_wizard_subheader" parent="@style/opt_in_wizard_item">
        <item name="android:textSize">@dimen/qp_opt_in_subheader_text_size</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:lineSpacingMultiplier">@dimen/qp_h4_line_spacing_multiplier</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="opt_in_wizard_item">
        <item name="android:layout_marginLeft">@dimen/qp_opt_in_horizontal_margin</item>
        <item name="android:layout_marginRight">@dimen/qp_opt_in_horizontal_margin</item>
        <item name="android:layout_marginStart">@dimen/qp_opt_in_horizontal_margin</item>
        <item name="android:layout_marginEnd">@dimen/qp_opt_in_horizontal_margin</item>
    </style>


    <style name="setup_wizard_display1">
        <item name="android:textSize">34.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#8affffff</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="setup_wizard_text_medium">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">#8affffff</item>
    </style>
    <style name="FirstRunText">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/text</item>
        <item name="android:textColorLink">@color/link_text</item>
        <item name="android:background">@null</item>
        <item name="android:layout_marginLeft">@dimen/first_run_padding</item>
        <item name="android:layout_marginRight">@dimen/first_run_padding</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:layout_marginStart">@dimen/first_run_padding</item>
        <item name="android:layout_marginEnd">@dimen/first_run_padding</item>
    </style>
    <style name="FirstRunTitle" parent="@style/FirstRunText">
        <item name="android:textSize">36.0dip</item>
        <item name="android:textColor">@color/text</item>
        <item name="android:textColorLink">@color/link_text</item>
        <item name="android:layout_marginTop">@dimen/first_run_padding</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
    </style>
    <style name="FirstRunSubtitle" parent="@style/FirstRunText">
        <item name="android:textSize">18.0dip</item>
        <item name="android:textColor">@color/text</item>
        <item name="android:textColorLink">@color/link_text</item>
        <item name="android:layout_marginTop">8.0dip</item>
        <item name="android:layout_marginBottom">8.0dip</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>


    <style name="EntryItemTitleText" parent="@android:style/TextAppearance.Medium">
        <item name="android:textColor">@color/text</item>
        <item name="android:textColorLink">@color/link_text</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLines">1</item>
    </style>
    <style name="EntryItemSubText" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">@color/text</item>
        <item name="android:textColorLink">@color/textt</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLines">1</item>
    </style>
    <style name="UnconfirmedEntryText" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">@color/text</item>
        <item name="android:textColorLink">@color/textt</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="TimeToLeaveTitleMessage" parent="@android:style/Widget.Material.TextView">
        <item name="android:textSize">@dimen/card_action_text</item>
        <item name="android:textStyle">italic</item>
        <item name="android:textColor">@color/card_title_text</item>
        <item name="android:paddingLeft">16.0dip</item>
        <item name="android:paddingTop">@dimen/quarter_unit</item>
        <item name="android:paddingBottom">@dimen/quarter_unit</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
    </style>
    <style name="TextOrIconDefaultStyle" parent="@style/CardText">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/text</item>
    </style>
    <style name="CardText">
        <item name="android:textSize">@dimen/card_small_text</item>
        <item name="android:textColor">@color/card_text</item>
        <item name="android:textColorLink">@color/link_text</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">2</item>
    </style>
    <style name="DefaultPagerSelectionDotsView">
        <item name="dotRadius">3.0dip</item>
        <item name="dotPadding">6.0dip</item>
        <item name="dotColor">#4dffffff</item>
        <item name="selectionRadius">6.0dip</item>
        <item name="selectionColor">#8cffffff</item>
    </style>


    <style name="Relationship.EmptyTitle" parent="@style/Relationship">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/text</item>
    </style>
    <style name="Relationship.EmptySubTitle" parent="@style/Relationship">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/textt</item>
    </style>

    <style name="Relationship">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">Roboto</item>
    </style>


    <style name="TravelModeSpinner" parent="@android:style/Widget.Material.Spinner">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">48.0dip</item>
        <item name="android:divider">@color/gsa_light_gray</item>
        <item name="android:dividerHeight">1.0dip</item>
    </style>
    <style name="EditReminderCustomActionButton"
        parent="@android:style/Widget.Material.ActionButton">
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
        <!--<item name="android:background">@drawable/stateful_colored_background_no_hairline</item>-->
        <item name="android:focusable">true</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1.0</item>
    </style>


    <style name="Theme.Velvet.Card.TextAppearance.ActionFieldValue"
        parent="@style/Theme.Velvet.Card.TextAppearance.Medium">
        <item name="android:textColor">@color/text</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/unit</item>
        <item name="android:layout_marginRight">@dimen/unit</item>
        <item name="android:layout_marginBottom">@dimen/half_unit</item>
        <item name="android:layout_marginStart">@dimen/unit</item>
        <item name="android:layout_marginEnd">@dimen/unit</item>
    </style>
    <style name="Theme.Velvet.Card.TextAppearance.Medium"
        parent="@android:style/TextAppearance.Medium">
        <item name="android:textSize">25.0sp</item>
        <item name="android:textColor">@color/text</item>
        <item name="android:fontFamily">sans-serif-light</item>
    </style>
    <style name="Closet.ActionButton.Overflow"
        parent="@android:style/Widget.Material.ActionButton.Overflow" />
    <style name="KeepPopupMenuStyle" parent="@android:style/Widget.Material.ListPopupWindow">
        <item name="android:background">@color/background</item>
    </style>
    <style name="KeepListViewDropdownStyle" parent="@android:style/Widget.ListView.DropDown">
        <item name="android:background">@color/background</item>
        <item name="android:listSelector">?android:listChoiceBackgroundIndicator</item>
    </style>
    <style name="horizontal_divider">
        <item name="android:background">#22ffffff</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1.0dip</item>
    </style>
    <style name="reminder_edit_spinner" parent="@android:style/Widget.Material.Spinner">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/text</item>
        <item name="android:textColorHint">@color/textt</item>
        <item name="android:layout_marginLeft">12.0dip</item>
        <item name="android:layout_marginRight">12.0dip</item>
    </style>

    <style name="Theme.Velvet.Card.TextAppearance.Small"
        parent="@android:style/TextAppearance.Small">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/text</item>
    </style>
    <style name="Theme.Velvet.Card.TextAppearance.Tiny"
        parent="@android:style/TextAppearance.Small">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@color/text</item>
    </style>

    <style name="VoiceOfGoogle">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">italic</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textColorLink">@android:color/black</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">16.0dip</item>
        <item name="android:paddingRight">16.0dip</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
    </style>

    <style name="Theme.HandsFree" parent="@android:style/Theme.Material">
        <item name="android:windowBackground">@color/background</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowActionBar">false</item>
    </style>

    <style name="SearchPlate">
        <item name="android:background">@android:color/black</item>
        <item name="android:backgroundTint">@android:color/black</item>
    </style>

    <style name="SearchPlate.SearchBox" parent="@android:style/Widget.EditText">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:backgroundTint">@android:color/black</item>
    </style>
